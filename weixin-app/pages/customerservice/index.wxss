movable-area {
  width: 100%;
  height: 100%;
  /*设置透明不影响后面的操作*/
  pointer-events: none;
  position: fixed;
  text-align: center;
  right: 0px;
  top: 0px;
  background: none;
}

.kefu,
.kefuIcon {
  width: 45px;
  height: 45px;
  border: none;
  padding: 0;
  background: none;
}

.kefu image {
  max-width: 100%;
  max-height: 100%;
}

.kefu button::after {
  border: none;
}