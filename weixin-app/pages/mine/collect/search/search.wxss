.searchContainer {
  height: 100%;
  width: 750rpx;
}
.top {
  display: flex;
  margin-top: 20rpx;
}

.search {
  display: inline-flex;
  align-items: center;
  margin: 0 20rpx 0 30rpx;
  padding: 10rpx 30rpx 10rpx 26rpx;
  width: 540rpx;
  height: 40rpx;
  background-color: #F5F5F5;
  /* border: 1rpx solid #BBBBBB; */
  border-radius: 40rpx;
  font-size: 26rpx;
}

.search_history {
  margin: 40rpx 30rpx 0 30rpx;
}
.title {
  display: flex;
  justify-content: space-between;
  padding: 0px 28rpx 0 0;
  
}
.box_title{
  padding-left: 30rpx;
  font-weight: 600;
}
.title_text {
  font-weight: 800;
}
.iconlajitong {
  font-size: 30rpx;
  color: #A3A3A3;
}
.search_history_content {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin: 20rpx 0 10rpx 0rpx;
}
.cell {
  margin: 10rpx 15rpx 10rpx 0rpx;
  background-color: #F5F5F5;
  padding: 1rpx 20rpx;
  border-radius: 15rpx;
  text-align: center;
  /* 超出最大宽度的展示省略号 */
  max-width: 250rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}


.iconfangdajing {
  font-size: 36rpx;
  color: #BFBFBF;
}

.input {
  flex: 1;
  margin-left: 20rpx;
}

.placeholder {
  color: #CFCFCF;
}

.bj {
  position: relative;
  left: 5vw;
  top: 8vh;
  width: 83vw;
  height: 104vh;
  border-radius: 20rpx;
  /* background-color: #9932CC	; */
  z-index: 0;
}

.ssd {
  position: relative;
  top: -3vh;
  width: 90vw;
  height: 20vh;
  margin-bottom: 50rpx;
  background-color: #FFFFFF;
  z-index: 1;
  box-shadow: 8rpx 8rpx 15rpx #000000;
  border-radius: 15rpx;
}
.box{
  display: flex;
  flex-direction: column;
  position: relative;
  left: 20rpx;
  top: -140rpx;
  width: 440rpx;
  height: 250rpx;
}
.ssd_img {
  position: relative;
  left: 67vw;
  top: 4vh;
  width: 150rpx;
  height: 150rpx;
  border-radius: 90rpx;
  background-color: pink;
}

.ssd_text {
  position: relative;
  left: 4vw;
  top: -11vh;
}

.ssd_collect {
  position: relative;
  left: 46vw;
  top: -36vh; 
}


 .content {
  background: #FFFFFF;
  padding: 20rpx 32rpx 0;
  display: flex;
  flex-direction: column;
  flex: 1;
  overflow: hidden;
}


.bottomHint {
  text-align: center;
}

.collection {
  width: 50rpx;
  height: 50rpx;
  margin-right: 5rpx;
}

.collection image {
  width: 100%;
  height: 100%;
}

.favorite {
  margin-right: 20rpx;
}

.noData {
  width: 686rpx;
  height: 686rpx;
}

.noData>image {
  width: 100%;
  height: 100%;
}

.noDataText {
  font-size: 28rpx;
  color: #333333;
  font-weight: 400;
  text-align: center;
} 